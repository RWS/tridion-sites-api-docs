{
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "apiconcepts/**.md",
          "apiconcepts/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/*.json",
          "**/*.html"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "filemetadata": {
      "langs": {
        "api/**.yml": [ "csharp", "vb", "fsharp", "cpp" ]
      }
    },
    "template": [
      "RWSTemplate"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_enableSearch": true
    },
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}